<style>
  svg path {
    opacity: 0;
  }
</style>

<script>
  window.onload = () => {
    const paths = document.querySelectorAll("path");
    var counter = 0;
    var pathAmount = paths.length + 1;
    svgPathFade();

    function svgPathFade() {
      let path: SVGPathElement = paths[counter];
      counter++;
      if (counter < pathAmount) {
        path.style.opacity = "1";
        setTimeout(svgPathFade, 100);
      }
    }
  };
</script>
